
<%- include("partials/header"); -%>
    <h1><strong>POST</strong></h1>
    <form class="" action="/compose" method="post">
      <div class="form-group">
        <br>
        <label>Title</label>
        <br>
        <input class="form-control" type="text" name="postTitle" autocomplete="off" required>
        <br>
        <label>Content</label>
        <br>
        <textarea class="form-control" name="postBody" rows="5" cols="30" required></textarea>
      </div>
      <button class="btn btn-primary" type="submit" name="button">Publish</button>
    </form>

<%- include("partials/footer"); -%>
